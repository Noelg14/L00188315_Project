{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "kvSettings": {
    "clientId": "",
    "clientSecret": "",
    "scope": "",
    "kvUrl": ""
  },
  "token": {
    "key": "",
    "issuer": ""
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      //{
      //  "Name": "Console"
      //},
      {
        "Name": "File",
        "Args": {
          "path": "logs\\Serilog.log",
          "rollingInterval": "Day"
        }
      }
    ],
    "Properties": {
      "Application": "SerilogVSNlog"
    }
  }
}
